fmod MYINT is
	sort Int .
	op 0 : -> Int .
	op s_ : Int -> Int .
	op p_ : Int -> Int .
	op _+_ : Int Int -> Int .
	op _-_ : Int Int -> Int .
	op _*_ : Int Int -> Int .
	vars N, M : Int .
	eq 0 + M = M .
	eq s p N = N .
	eq p s N = n .
	eq s N + M = s(N + M) .
	eq p N + M = p(N + M) .
	eq N - 0 = N
	eq N - s M = p(N - M) .
	eq N - p M = s(N - M) .
	
endfm